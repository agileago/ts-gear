{
  "name": "ts-gear",
  "version": "1.3.1",
  "description": "swagger to typescript",
  "main": "lib/index.js",
  "bin": {
    "tsg": "bin/main.js",
    "ts-gear": "bin/main.js"
  },
  "scripts": {
    "test": "cross-env NODE_ENV=test jest",
    "coverage": "cross-env NODE_ENV=test jest --coverage && open-cli coverage/lcov-report/index.html -- google-chrome",
    "compile": "cross-env tsc --esModuleInterop --resolveJsonModule -d src/*.ts --outDir bin && chmod +x bin/main.js",
    "release": "standard-version",
    "prepublishOnly": "yarn run-s test compile release"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/superwf/ts-gear.git"
  },
  "dependencies": {
    "axios": "^0.19.2",
    "chalk": "^2.4.2",
    "commander": "^4.1.0",
    "cosmiconfig": "^5.2.1",
    "fs-extra": "^7.0.1",
    "isomorphic-fetch": "^2.2.1",
    "jest-html-reporter": "^2.5.0",
    "json-schema": "^0.2.3",
    "lodash": "^4.17.15",
    "path-to-regexp": "^3.0.0",
    "prettier": "^1.18.2",
    "reserved-words": "^0.1.2",
    "swagger-schema-official": "^2.0.0-bab6bed",
    "translation.js": "^0.7.13",
    "traverse": "^0.6.6",
    "ts-morph": "^7.0.1",
    "ts-node": "^8.8.2",
    "typescript": "^3.8.3",
    "url-join": "^4.0.1"
  },
  "files": [
    "src",
    "*.md",
    "*.json",
    "*.js",
    "bin"
  ],
  "devDependencies": {
    "@babel/cli": "^7.8.4",
    "@babel/core": "^7.9.0",
    "@babel/preset-env": "^7.9.5",
    "@babel/preset-typescript": "^7.9.0",
    "@commitlint/config-conventional": "^8.1.0",
    "@commitlint/prompt-cli": "^8.1.0",
    "@types/chalk": "^2.2.0",
    "@types/cosmiconfig": "^5.0.3",
    "@types/fs-extra": "^5.0.5",
    "@types/http-server": "^0.10.0",
    "@types/isomorphic-fetch": "^0.0.35",
    "@types/jest": "^24.0.12",
    "@types/json-schema": "^7.0.3",
    "@types/lodash": "^4.14.110",
    "@types/node": "^11.13.8",
    "@types/node-fetch": "^2.3.3",
    "@types/prettier": "^1.16.3",
    "@types/rimraf": "^2.0.3",
    "@types/swagger-schema-official": "^2.0.21",
    "@types/traverse": "^0.6.25-alpha",
    "@types/url-join": "^4.0.0",
    "@typescript-eslint/eslint-plugin": "^2.9.0",
    "@typescript-eslint/parser": "^2.9.0",
    "babel-eslint": "^10.0.3",
    "babel-jest": "^25.4.0",
    "babel-plugin-module-resolver": "^4.0.0",
    "commitlint": "^8.1.0",
    "cross-env": "^5.2.0",
    "eslint": "^6.7.1",
    "eslint-config-airbnb-base": "^14.1.0",
    "eslint-config-prettier": "^6.1.0",
    "eslint-import-resolver-alias": "^1.1.2",
    "eslint-plugin-babel": "^5.3.0",
    "eslint-plugin-import": "^2.20.2",
    "eslint-plugin-jest": "^22.16.0",
    "eslint-plugin-prettier": "^3.1.0",
    "http-server": "^0.11.1",
    "husky": "^3.0.2",
    "jest": "^24.9.0",
    "npm-run-all": "^4.1.5",
