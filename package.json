{
  "name": "ts-gear",
  "version": "3.0.4",
  "description": "swagger to typescript with mock data",
  "main": "lib/index.js",
  "bin": {
    "tsg": "lib/main.js",
    "ts-gear": "lib/main.js"
  },
  "scripts": {
    "test": "cross-env NODE_ENV=test jest",
    "coverage": "cross-env NODE_ENV=test jest --coverage | ts-node script/coverageReportToMarkdown.ts && jest-coverage-badges output ./badges && open-cli coverage/lcov-report/index.html -- google-chrome",
    "build": "cross-env rimraf lib && cross-env tsc -p tsconfig.build.json && chmod +x lib/main.js",
    "release": "standard-version",
    "prepublishOnly": "yarn run-s test build release"
  },
  "types": "./lib/index.d.ts",
  "repository": {
    "type": "git",
    "url": "https://github.com/superwf/ts-gear.git"
  },
  "dependencies": {
    "axios": "^0.19.2",
    "chalk": "^2.4.2",
    "commander": "^4.1.0",
    "cosmiconfig": "^5.2.1",
    "cross-fetch": "^3.0.6",
    "fs-extra": "^7.0.1",
    "json-schema": "^0.2.3",
    "lodash": "^4.17.20",
    "mkdirp": "^1.0.4",
    "openapi3-ts": "^1.4.0",
    "path-to-regexp": "^3.0.0",
    "prettier": "^2.0.5",
    "prompts": "^2.3.2",
    "swagger-schema-official": "^2.0.0-bab6bed",
    "translation.js": "^0.7.13",
    "traverse": "^0.6.6",
    "ts-morph": "^7.0.3",
    "ts-node": "^8.8.2",
    "tsconfig-paths": "^3.9.0",
    "typescript": "^4.0.2",
    "url-join": "^4.0.1",
    "whatwg-fetch": "^3.4.1"
  },
  "files": [
    "*.md",
    "lib"
  ],
  "devDependencies": {
    "@commitlint/config-conventional": "^11.0.0",
    "@commitlint/prompt-cli": "^11.0.0",
    "@types/chalk": "^2.2.0",
    "@types/cosmiconfig": "^5.0.3",
    "@types/fs-extra": "^5.0.5",
    "@types/http-server": "^0.10.0",
    "@types/jest": "^26.0.19",
    "@types/json-schema": "^7.0.3",
    "@types/lodash": "^4.14.110",
    "@types/mkdirp": "^1.0.0",
    "@types/moxios": "^0.4.9",
    "@types/node": "^11.13.8",
    "@types/prettier": "^2.0.0",
    "@types/prompts": "^2.0.5",
    "@types/rimraf": "^2.0.3",
    "@types/swagger-schema-official": "^2.0.21",
    "@types/traverse": "^0.6.25-alpha",
    "@typescript-eslint/eslint-plugin": "^4.10.0",
    "@typescript-eslint/parser": "^4.10.0",
    "commitlint": "^11.0.0",
    "cross-env": "^5.2.0",
    "eslint": "^7.5.0",
    "eslint-config-airbnb-base": "^14.2.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-import-resolver-alias": "^1.1.2",
    "eslint-plugin-import": "^2.22.0",
    "eslint-plugin-jest": "^24.1.3",
    "eslint-plugin-prettier": "^3.1.4",
    "fetch-mock": "^9.10.7",
    "husky": "^4.3.0",
    "jest": "^26.6.3",
    "jest-coverage-badges": "^1.1.2",
    "moxios": "^0.4.0",
    "npm-run-all": "^4.1.5",
    "open-cli": "^6.0.1",
    "readline": "^1.3.0",
    "rimraf": "^3.0.0",
    "standard-version": "^8.0.2",
    "ts-jest": "^26.4.4"
  },
  "keywords": [
    "openapi",
    "swagger",
    "typescript",
    "json schema",
    "ts-gear",
    "code generator",
    "interface",
    "structure"
  ],
  "husky": {
    "hooks": {
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
    }
  },
  "author": "superwf",
  "license": "MIT"
}
