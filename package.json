    "json schema",
    "ts-gear",
    "typescript"
  ],
  "husky": {
    "hooks": {
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
    }
  },
  "author": "superwf",
  "license": "MIT"
}
